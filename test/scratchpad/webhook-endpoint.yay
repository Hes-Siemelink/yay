On Http request:
  path: /github/<something>
  Do:
    Print: ${output}

---
On Http request:
  path: /shutdown
  Do:
    Stop webhook: *

---
On Http request:
- path: /document/{document_id}
  method:
  - PUT
  - POST
  Do:
    Store:
      in: documents
      key: ${document_id}
      value: ${input}
- path: /document/{document_id}
  method: GET
  Do:
    Retrieve:
      from: documents
      key: ${document_id}

---
On Http request:
- PUT:  /document/{document_id}?name={name}
  POST: /document/{document_id}
  Do:
    Store:
      in: documents
      key: ${document_id}
      value: ${input}
- GET: /document/{document_id}
  Do:
    Retrieve:
      from: documents
      key: ${document_id}

---
On Http request:
- /document/{document_id}:
    PUT:
      Store:
        in: documents
        key: ${document_id}
        value: ${input}
    GET:
      Retrieve:
        from: documents
        key: ${document_id}

---
On Http request:
- /user/{userId}:
    get:
      summary: Returns a user by ID.
      parameters:
        - name: userId
          in: path
          required: true
          description: Parameter description in CommonMark or HTML.
          schema:
            type : integer
            format: int64
            minimum: 1
      responses:
        '200':
          description: OK
      Do:
        Retrieve:
          from: users
          key: ${userId}
